# Google Cloud Vertex AI (Primary AI & Grounding)
# Note: GOOGLE_API_KEY is no longer used (replaced by GCP_PROJECT_ID)
GCP_PROJECT_ID=your_gcp_project_id
GCP_LOCATION=us-central1
GOOGLE_MODEL_NAME=gemini-2.5-pro

# DeepSeek API Configuration (for CN/HK stocks)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_MODEL_NAME=deepseek-chat
DEEPSEEK_BASE_URL=https://api.deepseek.com

# Qwen API Configuration (alternative for CN/HK stocks)
QWEN_API_KEY=your_qwen_api_key_here
QWEN_MODEL_NAME=qwen-plus
QWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# Search Provider Configuration
# Options: "vertex" (Google Grounding), "tavily", or "serper"
# Vertex: Uses Google Search via Vertex AI (requires GCP_PROJECT_ID)
# Tavily: Uses Tavily API (requires TAVILY_API_KEY)
# Serper: Uses Serper.dev API (requires SERPER_API_KEY)
SEARCH_PROVIDER=vertex

# Serper.dev API (Google Search API wrapper)
SERPER_API_KEY=your_serper_api_key_here

# Tavily API (alternative search provider)
TAVILY_API_KEY=your_tavily_api_key_here
